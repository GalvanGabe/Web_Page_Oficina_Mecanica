<section class="registered-customers py-4">
    <nav class="text-white bg-gradient ps-3" aria-label="breadcrumb">
        <ol class="breadcrumb breadcrumb-chevron rounded-3">
            <li class="breadcrumb-item">
                <i class="fa-solid fa-warehouse fa-xl"></i>
                <span class="visually-hidden">Home</span>
            </li>
            <li class="breadcrumb-item">
                <a class="text-white fw-semibold text-decoration-none" href="/">Painel de Servi√ßos</a>
            </li>
            <li class="text-white breadcrumb-item active" aria-current="page">Clientes Cadastrados</li>
        </ol>
    </nav>
    <div class="container">
      <div class="bd-example-snippet bd-code-snippet">
        <div class="bd-example m-0 border-0">
          <table class="table table-striped">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Nome</th>
                <th scope="col">CPF</th>
                <th scope="col">Email</th>
                <th scope="col">Telefone</th>
              </tr>
            </thead>
            <tbody>
              {{#each customers}}
                <tr>
                  <th scope="row"><i class="fa-solid fa-user fa-lg"></i> {{this.id}}</th>
                  <td><a href="/customer/{{this.id}}">{{this.name}}</a></td>
                  <td>{{this.cpf}}</td>
                  <td>{{this.email}}</td>
                  <td>{{this.phone}}</td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>   
</section>