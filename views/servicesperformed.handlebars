<section class="services-performed py-4">
    <nav class="bg-gradient ps-3" aria-label="breadcrumb">
        <ol class="breadcrumb breadcrumb-chevron rounded-3">
            <li class="breadcrumb-item">
                <i class="fa-solid fa-warehouse fa-xl"></i>
                <span class="visually-hidden">Home</span>
            </li>
            <li class="breadcrumb-item">
                <a class="fw-semibold text-decoration-none" href="/">Painel de Serviços</a>
            </li>
            <li class="breadcrumb-item active" aria-current="page">Serviços Realizados</li>
        </ol>
    </nav>
    <div class="container">
        <div class="bd-example-snippet bd-code-snippet">
            <div class="bd-example m-0 border-0">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Serviço</th>
                            <th scope="col">KM</th>
                            <th scope="col">Data</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each services}}
                            <tr>
                                <th scope="row"><i class="fa-solid fa-screwdriver-wrench fa-lg"></i> {{this.id}}</th>
                                <td>{{this.type}}</td>
                                <td>{{this.km}}</td>
                                <td>{{this.date}}</td>
                            </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>